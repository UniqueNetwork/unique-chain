version: "3.5"

services:
  try-runtime:
    image: uniquenetwork/tryruntime-{{ NETWORK }}-testnet-local:latest
    container_name: tryruntime-{{ NETWORK }}-testnet-local
    expose:
      - 9944
      - 9933
    ports:
      - 127.0.0.1:9944:9944
      - 127.0.0.1:9933:9933
    logging:
      options:
        max-size: "1m"
        max-file: "3"

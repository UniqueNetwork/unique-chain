{
	"exclude": [
		"node_modules",
		"node_modules/**/*",
		"../node_modules/**/*",
		"**/node_modules/**/*"
	],
	"compilerOptions": {
		"target": "ES2020",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"module": "commonjs",
		"sourceMap": true,
		"outDir": ".",
		"rootDir": ".",
		"strict": true,
		"paths": {
		},
		"skipLibCheck": true,
	},
	"include": [
		"**/*",
	],
	"lib": [
		"es2017"
	],
}
